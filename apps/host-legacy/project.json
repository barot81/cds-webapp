{
  "projectType": "application",
  "sourceRoot": "apps/host-legacy/src",
  "prefix": "zhealthcare.ui.packages",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/host-legacy",
        "index": "apps/host-legacy/src/index.html",
        "main": "apps/host-legacy/src/main.ts",
        "polyfills": "apps/host-legacy/src/polyfills.ts",
        "tsConfig": "apps/host-legacy/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/host-legacy/src/favicon.ico",
          "apps/host-legacy/src/assets",
          {
            "glob": "**/*",
            "input": "libs/legacy/assets/global/",
            "output": "./assets/"
          }
        ],
        "styles": [
          "apps/host-legacy/src/styles.scss",
          {
            "input": "libs/legacy/ux/src/lib/styles/themes/_blue-theme.scss",
            "inject": false,
            "bundleName": "theme-blue"
          },
          {
            "input": "libs/legacy/ux/src/lib/styles/themes/_purple-theme.scss",
            "inject": false,
            "bundleName": "theme-purple"
          },
          "node_modules/@fortawesome/fontawesome-pro/css/all.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/host-legacy/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/host-legacy/src/environments/environment.ts",
              "with": "apps/host-legacy/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/host-legacy/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:module-federation-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "host-legacy:build:production"
        },
        "development": {
          "browserTarget": "host-legacy:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 5200,
        "publicHost": "http://localhost:5200"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "host-legacy:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/host-legacy/**/*.ts",
          "apps/host-legacy/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/host-legacy"],
      "options": {
        "jestConfig": "apps/host-legacy/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
